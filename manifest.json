{
  "name": "Deepfrier",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Deepfry pages",
  "icons": {
		"128": "icon128.png"
	},
  "browser_action": {
    "default_title": "Deepfry!",
    "default_icon":	{ "19": "icon19.png", "38": "icon38.png", "48": "icon48.png", "128": "icon128.png" },
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}